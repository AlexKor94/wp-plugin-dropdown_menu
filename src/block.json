{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "apriltwentythree/sub-menu",
  "title": "Dropdown menu",
  "category": "theme",
  "icon": "menu",
  "description": "Adds a cutom dropdown menu",
  "keywords": ["sidebar", "dropdown", "menu", "list"],
  "version": "1",
  "textdomain": "apriltwentythree",
  "attributes": {
  "items": {
    "type": "array",
    "source": "children",
    "selector": "ul",
    "default": []
  },
   "title": {
      "type":"string",
      "default":""
      },
    "links":{
      "type":"array",
      "default":["1"]
      },
    "type":{
      "type":"string",
      "default":"unordered"
      }
  },
  "editorScript":"file:./index.js",
  "viewScript":""
}